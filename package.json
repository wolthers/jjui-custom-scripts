{
  "name": "jjui-custom-scripts",
  "version": "0.1.0",
  "description": "CLI for jj/jjui workflows: PRs, stack integrate/restack, workspaces",
  "type": "module",
  "bin": {
    "jj-scripts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "lint": "oxlint --deny-warnings src",
    "lint:fix": "oxlint --deny-warnings --fix src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "jj",
    "jjui",
    "jujutsu",
    "cli"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4",
  "dependencies": {
    "commander": "~14.0.3",
    "execa": "~9.6.1"
  },
  "devDependencies": {
    "@types/node": "~25.3.2",
    "oxlint": "~1.50.0",
    "tsup": "~8.5.1",
    "tsx": "~4.21.0",
    "typescript": "~5.9.3",
    "vitest": "~2.1.9"
  }
}
